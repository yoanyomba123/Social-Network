	body.landing
		#page-wrapper
			// Header
			header#header.alt
				nav#nav.sticky-top
					if !user
						ul
							li
								a.p-2(href='/') Home
							li
								a.p-2(href='/markets') Markets
							li
								a.p-2(href='/economy') Economy
							li
								a.p-2(href='/opinions') Commentary
							li
								a.p-2(href='/filings') Fund Filings
							li
								a.p-2(href='/crypto') Crypto-Currencies
							li
								a.button(href='/register') Sign Up
							li
								a.button(href='/login') Login
					else
						ul
							li
								a(href='/homefeed') My Feed
							li
								a(href='/portfolio') My Portfolio
							li
								a(href='/news') My News
			!= messages('message', locals)
			if errors  
				each error, i in errors
					div(class="alert alert-danger") #{error.msg}
			block content
			// Footer
			footer#footer
				ul.icons
					li
						a.icon.fa-twitter(href='#')
							span.label Twitter
					li
						a.icon.fa-facebook(href='#')
							span.label Facebook
					li
						a.icon.fa-instagram(href='#')
							span.label Instagram
					li
						a.icon.fa-github(href='#')
							span.label Github
					li
						a.icon.fa-dribbble(href='#')
							span.label Dribbble
					li
						a.icon.fa-google-plus(href='#')
							span.label Google+
				ul.copyright
					li Â© Untitled. All rights reserved.
					li
						| Design: 
						a(href='http://html5up.net') HTML5 UP
